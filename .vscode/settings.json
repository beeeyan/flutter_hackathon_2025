{
  "dart.flutterSdkPath": ".fvm/versions/3.35.7",
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "files.associations": {
    "*.kts": "kotlin"
  },
  "[dart]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "cSpell.words": [
    "Myacle",
    "Myaku"
  ]
}